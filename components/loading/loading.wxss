.wrap-box {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(255, 255, 255, 0);
  z-index: 1000;
}

.wrapper {
  width: 264rpx;
  height: 232rpx;
  overflow: auto;
  margin: auto;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -81rpx;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  /* background: #f5f6fa; *//* background: rgba(21, 21, 21, .75);  */
  border-radius: 4rpx;
  /* transition: all 1s ease; */
}

.wrapper>view {
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
  height: 100%;
}

.wrapper image {
  width: 100rpx;
  height: 100rpx;
}

.wrapper text {
  font-size: 24rpx;
  margin-top: 10rpx;
  text-align: center;
  color: #fff;
}

.vux-loading .weui-toast {
  z-index: 5001;
}

.weui-toast {
  /* -webkit-transform: translateX(-50%);
    transform: translateX(-50%); */
  width: 100%;
  height: 100%;
  background: hsla(0, 0%, 7%, 0.7);
  text-align: center;
  border-radius: 10rpx;
  color: #fff;
}

.weui-icon_toast.weui-loading, .weui-icon_toast.weui-finish,
.weui-icon_toast.weui-warning, .weui-icon_toast.weui-waitting {
  margin: 66rpx 0 0;
  width: 76rpx;
  height: 76rpx;
  vertical-align: baseline;
  display: inline-block;
}

.weui-loading {
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-finish {
  /* animation: weuiLoading 1s steps(12, end) infinite; */
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.weui-warning {
  /* animation: weuiLoading 1s steps(12, end) infinite; */
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.weui-waitting {
  /* animation: weuiLoading 1s steps(12, end) infinite; */
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.vux-mask-enter, .vux-mask-leave-active, .vux-mask-leave-active,
.vux-mask-enter-active {
  position: relative;
  z-index: 1;
}

.vux-mask-enter, .vux-mask-leave-active {
  opacity: 0;
}

.vux-mask-leave-active, .vux-mask-enter-active {
  transition: opacity 300ms;
}

.vux-loading-no-text .weui-toast {
  min-height: 98px;
}

.weui-toast__content {
  font-size: 32rpx;
  margin: 0 0 30rpx;
}
