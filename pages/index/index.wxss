}

.notice-wrap {
  height: 72rpx;
  line-height: 72rpx;
  background-color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.easyfun-wrap {
  height: 200rpx;
